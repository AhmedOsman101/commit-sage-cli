{
  "version": "1.7.0",
  "tasks": {
    "dev": "deno run -A --watch src/main.ts",
    "run": "deno run -A src/main.ts",
    "format": "pnpm dlx @biomejs/biome@2.3.11 check --fix .",
    "format:check": "pnpm dlx @biomejs/biome@2.3.11 check .",
    "compile": "deno compile -A -o $HOME/.local/bin/commit-sage src/main.ts",
    "compile-dev": "deno compile -A -o $HOME/scripts/bin/commit-sage src/main.ts",
    "compile-windows": "deno compile -A -o bin/commit-sage.exe --target x86_64-pc-windows-msvc src/main.ts",
    "compile-macos-x64": "deno compile -A -o bin/commit-sage --target x86_64-apple-darwin src/main.ts",
    "compile-macos-arm64": "deno compile -A -o bin/commit-sage --target aarch64-apple-darwin src/main.ts",
    "compile-linux-x64": "deno compile -A -o bin/commit-sage --target x86_64-unknown-linux-gnu src/main.ts",
    "compile-linux-arm64": "deno compile -A -o bin/commit-sage --target aarch64-unknown-linux-gnu src/main.ts"
  },
  "imports": {
    "@ai-sdk/anthropic": "npm:@ai-sdk/anthropic@^3.0.47",
    "@ai-sdk/deepseek": "npm:@ai-sdk/deepseek@^2.0.20",
    "@ai-sdk/google": "npm:@ai-sdk/google@^3.0.33",
    "@ai-sdk/mistral": "npm:@ai-sdk/mistral@^3.0.20",
    "@ai-sdk/openai": "npm:@ai-sdk/openai@^3.0.36",
    "@ai-sdk/xai": "npm:@ai-sdk/xai@^3.0.59",
    "@arrirpc/schema": "npm:@arrirpc/schema@^0.81.2",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0",
    "@std/fmt": "jsr:@std/fmt@1.0.9",
    "ai": "npm:ai@^6.0.103",
    "axios": "npm:axios@1.13.5",
    "husky": "npm:husky@^9.1.7",
    "lib-result": "npm:lib-result@^3.2.2",
    "ollama-ai-provider-v2": "npm:ollama-ai-provider-v2@^3.3.1",
    "@/": "./src/"
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-unused-vars",
        "no-explicit-any",
        "ban-types",
        "no-fallthrough"
      ]
    }
  },
  "nodeModulesDir": "auto"
}
